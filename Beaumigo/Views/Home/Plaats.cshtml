@model Beaumigo.Models.Plaats
@{
    ViewData["Title"] = "Plaats";
}
<h1>@ViewData["id"]</h1>
<body>
    <h1>Onze heerlijke tacos zijn onder andere te vinden in deze straat : @Model.straatnummer</h1>
    <h2>De postcode is: @Model.postcode</h2>
    <h2>Je kunt ons bellen met het nummer : @Model.telefoonnummer </h2>
<p> door middel van onderstaande maps kun je onze locaties van onder andere in Leidschendam vinden: </p>
    <div class="mapouter"><div class="gmap_canvas"><iframe width="600" height="500" id="gmap_canvas" src="https://maps.google.com/maps?q=Leidschendam-Voorburg&t=&z=13&ie=UTF8&iwloc=&output=embed" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"></iframe><a href="https://123movies-to.org"></a><br>
    <style>
        .mapouter {
            position: relative;
            text-align: right;
            height: 500px;
            width: 600px;
        }
    </style><a href="https://www.embedgooglemap.net">embed google maps without iframe</a>
    <style>
        .gmap_canvas {
            overflow: hidden;
            background: none !important;
            height: 500px;
            width: 600px;
        }
    </style></div></div>

</body>