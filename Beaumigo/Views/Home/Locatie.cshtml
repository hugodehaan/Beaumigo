@model List<Locatie>
@{
    ViewData["Title"] = "Locatie";
}


<div class="locatie">
    <ul>
        @foreach (Locatie l in Model)
        {
            <li>
                <a class="goed" href="/straatnummer/@l.Id">@l.straatnummer</a>

                <br />
                <br />
                <br />
            </li>
        }
    </ul>
</div>

