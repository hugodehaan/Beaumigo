@model List<Eten>
@{
    ViewData["Title"] = "Eten";
}
<div class="slideshow">
    <ul>
        @foreach (Eten g in Model)
        {
            <li>
                <a class="goed" href="/gerecht/@g.Id">@g.Naam</a>
                

                <br />
                <br />
            </li>
        }
    </ul>
</div>

