@model List<Eten>
@{
    ViewData["Title"] = "Eten";
}
<div class="eten">
    <ul>
        @foreach (Eten g in Model)
        {
            <li>
                <a class="goed" href="/gerecht/@g.Id">@g.Naam   -€@g.Prijs</a>
                <p>@g.Ingredienten</p>
                

                <br />
                <br />
            </li>
        }
    </ul>
</div>

